<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.larrainvial.logviwer.controller.algos.LastPriceController">
  <children>

    <TableView fx:id="lastPrice">

      <columns>
        <TableColumn style="-fx-alignment: CENTER;" text="Last Price" fx:id="hour" prefWidth="90"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Type" fx:id="messageByType" prefWidth="50"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Symbol" fx:id="symbol" prefWidth="90"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Buy Px" fx:id="buyPx" prefWidth="70"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Buy Qty" fx:id="buyQty" prefWidth="70"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Sell Px" fx:id="sellPx" prefWidth="70"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Sell Qty" fx:id="sellQty" prefWidth="70"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Close Px" fx:id="closePx" prefWidth="70"/>
        <TableColumn style="-fx-alignment: CENTER;" text="Trade Amount" fx:id="tradeAmount" prefWidth="50"/>
      </columns>

    </TableView>
  </children>
</AnchorPane>
